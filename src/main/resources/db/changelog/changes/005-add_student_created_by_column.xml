<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <changeSet id="005-add_student_created_by_column" author="tomas.gramblicka">

        <addColumn tableName="student">
            <column name="created_by" type="BIGINT" defaultValue="4">
                <constraints nullable="false" foreignKeyName="fk_student_created_by_user" references="user(id)"/>
            </column>
        </addColumn>


        <rollback>
            <dropForeignKeyConstraint baseTableName="student" constraintName="fk_student_created_by_user"/>
            <dropColumn tableName="student" columnName="created_by"></dropColumn>
        </rollback>
    </changeSet>
</databaseChangeLog>
